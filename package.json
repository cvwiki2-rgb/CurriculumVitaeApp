{
  "name": "curriculumvitaeapp",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "lint:css": "stylelint \"src/**/*.{scss,css}\" --fix",
    "format": "eslint . --ext .ts,.tsx,.js,.jsx --fix && prettier --write .",
