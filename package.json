    "@mui/icons-material": "^7.3.5",
    "@mui/material": "^7.3.5",
    "classnames": "^2.5.1",
    "graphql": "^16.12.0",
    "i18next": "^25.6.2",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-hook-form": "^7.66.1",
    "react-i18next": "^16.3.0",
    "react-router": "^7.9.5",
    "rxjs": "^7.8.2"
